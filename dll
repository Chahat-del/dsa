#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct Emp {
    int ssn;
    char name[21], dept[21], desg[21];
    float sal;
    long phno;
    struct Emp *prev, *next;
} Emp;

Emp *head = NULL, *tail = NULL;
int count = 0;

void createEnd(int N) {
    while (N--) {
        Emp *node = (Emp *)malloc(sizeof(Emp));
        scanf("%d %20s %20s %20s %f %ld",
              &node->ssn, node->name, node->dept,
              node->desg, &node->sal, &node->phno);
        node->next = node->prev = NULL;

        if (head == NULL) {
            head = tail = node;
        } else {
            tail->next = node;
            node->prev = tail;
            tail = node;
        }
        count++;
    }
}

void display() {
    if (head == NULL) {
        printf("Linked list elements from begining :\n");
        printf("List empty to display\n");
        return;
    }

    Emp *temp = head;
    printf("Linked list elements from begining :\n");
    while (temp != NULL) {
        printf("%d %s %s %s %f %ld\n",
               temp->ssn, temp->name, temp->dept,
               temp->desg, temp->sal, temp->phno);
        temp = temp->next;
    }
    printf(" No of employees = %d\n", count);
}

void insertEnd() {
    Emp *node = (Emp *)malloc(sizeof(Emp));
    scanf("%d %20s %20s %20s %f %ld",
          &node->ssn, node->name, node->dept,
          node->desg, &node->sal, &node->phno);
    node->next = node->prev = NULL;

    if (head == NULL) {
        head = tail = node;
    } else {
        tail->next = node;
        node->prev = tail;
        tail = node;
    }
    count++;
}

void deleteEnd() {
    if (tail == NULL) {
        printf("wrong choice\n");
        return;
    }

    Emp *temp = tail;
    printf("%d %s %s %s %f %ld\n",
           temp->ssn, temp->name, temp->dept,
           temp->desg, temp->sal, temp->phno);

    if (head == tail) {
        head = tail = NULL;
    } else {
        tail = tail->prev;
        tail->next = NULL;
    }
    free(temp);
    count--;
}

void insertFront() {
    Emp *node = (Emp *)malloc(sizeof(Emp));
    scanf("%d %20s %20s %20s %f %ld",
          &node->ssn, node->name, node->dept,
          node->desg, &node->sal, &node->phno);
    node->next = node->prev = NULL;

    if (head == NULL) {
        head = tail = node;
    } else {
        node->next = head;
        head->prev = node;
        head = node;
    }
    count++;
}

void deleteFront() {
    if (head == NULL) {
        printf("wrong choice\n");
        return;
    }

    Emp *temp = head;
    printf("%d %s %s %s %f %ld\n",
           temp->ssn, temp->name, temp->dept,
           temp->desg, temp->sal, temp->phno);

    if (head == tail) {
        head = tail = NULL;
    } else {
        head = head->next;
        head->prev = NULL;
    }
    free(temp);
    count--;
}

int main() {
    int choice, N;
    while (scanf("%d", &choice) == 1) {
        switch (choice) {
        case 1:
            scanf("%d", &N);
            createEnd(N);
            break;
        case 2:
            display();
            break;
        case 3:
            insertEnd();
            break;
        case 4:
            deleteEnd();
            break;
        case 5:
            insertFront();
            break;
        case 6:
            deleteFront();
            break;
        case 7:
            return 0;
        default:
            printf("wrong choice\n");
        }
    }
    return 0;
}
